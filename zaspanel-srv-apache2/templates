Template: zaspanel-srv-apache2/server
Type: string
Default:
Description: MySQL Host for apache2

Template: zaspanel-srv-apache2/user
Type: string
Default:
Description: MySQL User for apache2

Template: zaspanel-srv-apache2/pass
Type: password
Default:
Description: MySQL password for apache2

Template: zaspanel-srv-apache2/database
Type: string
Default:
Description: MySQL Database for apache2

Template: zaspanel-srv-apache2/path
Type: string
Default: /var/www
Description: Base path for web files
