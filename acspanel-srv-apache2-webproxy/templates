Template: acspanel-srv-apache2-webproxy/server
Type: string
Default:
Description: MySQL Host for apache2 webproxy

Template: acspanel-srv-apache2-webproxy/user
Type: string
Default:
Description: MySQL User for apache2 webproxy

Template: acspanel-srv-apache2-webproxy/pass
Type: password
Default:
Description: MySQL password for apache2 webproxy

Template: acspanel-srv-apache2-webproxy/database
Type: string
Default:
Description: MySQL Database for apache2 webproxy

Template: acspanel-srv-apache2-webproxy/path
Type: string
Default: /var/www
Description: Base path for web files
